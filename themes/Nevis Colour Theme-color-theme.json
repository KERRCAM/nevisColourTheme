	"tokenColors": [
		{
		"scope": [
			"variable",
			"string constant.other.placeholder",
			"invalid.deprecated",
			"keyword",
			"storage.type",
			"storage.modifier"
		],
		"settings": {
			"foreground": "#C04878"
		}
		},
		{
		"scope": [
			"variable",
			"string constant.other.placeholder",
			"entity.name.function",
		],
		"settings": {
			"fontStyle": "italic"
		}
		},
		{
		"scope": [
			"constant.other.color"
		],
		"settings": {
			"foreground": "#C8C7FF"
		}
		},
		{
		"scope": [
			"invalid",
			"invalid.illegal"
		],
		"settings": {
			"foreground": "#D878A8"
		}
		},
		{
		"scope": [
			"Keyword",
			"Storage"
		],
		"settings": {
			"fontStyle": "italic"
		  }
		},
		{
		"scope": [
			"keyword.control",
			"constant.other.color",
